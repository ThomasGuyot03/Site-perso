r\n\r\n    <!-- Section des technologies -->\r\n    <section class=\"technology-strip\">\r\n      <div class=\"tech-container\" ref=\"techContainer\">\r\n        <div class=\"tech-item\" v-for=\"(tech, index) in technologies\" :key=\"index\">\r\n          <img :src=\"tech.src\" :alt=\"tech.name\" />\r\n          <span>{{ tech.name }}</span>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\n\r\n// Liste des technologies à afficher\r\nconst technologies = [\r\n  { name: 'HTML', src: 'https://cdn-icons-png.flaticon.com/512/732/732212.png' },\r\n  { name: 'CSS', src: 'https://cdn-icons-png.flaticon.com/512/732/732190.png' },\r\n  { name: 'Bulma', src: 'https://i.postimg.cc/m1Hxh2zL/icons8-bulma-io-48.png' },\r\n  { name: 'Vue.js', src: 'https://i.postimg.cc/8sPjs8zh/icons8-vue-js-48.png' },\r\n  { name: 'Node.js', src: 'https://cdn-icons-png.flaticon.com/512/5968/5968322.png' },\r\n  { name: 'Python', src: 'https://cdn-icons-png.flaticon.com/512/5968/5968350.png' },\r\n];\r\n\r\n// Référence au conteneur de la bande\r\nconst techContainer = ref(null);\r\n\r\n// Fonction de défilement fluide\r\nconst startScrolling = () => {\r\n  const container = techContainer.value;\r\n  const speed = 1; // Vitesse de défilement (plus petit = plus lent)\r\n\r\n  let scrollAmount = 0;\r\n\r\n  // Clone des éléments pour créer une boucle infinie\r\n  const cloneItems = () => {\r\n    const items = Array.from(container.children);\r\n    items.forEach((item) => {\r\n      const clone = item.cloneNode(true);\r\n      container.appendChild(clone);\r\n    });\r\n  };\r\n\r\n  cloneItems();\r\n\r\n  // Animation de défilement\r\n  const scroll = () => {\r\n    scrollAmount += speed;\r\n    if (scrollAmount >= container.scrollWidth / 2) {\r\n      scrollAmount = 0; // Réinitialisation pour éviter les saccades\r\n    }\r\n    container.style.transform = `translateX(-${scrollAmount}px)`;\r\n    requestAnimationFrame(scroll);\r\n  };\r\n\r\n  scroll();\r\n};\r\n\r\n// Lancement au montage du composant\r\nonMounted(() => {\r\n  startScrolling();\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  margin-top: 60px;\r\n  padding: 20px;\r\n  color: #fff;\r\n  text-align: center;\r\n}\r\n\r\n/* Section introduction */\r\n.intro {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.intro h1 {\r\n  font-size: 3rem;\r\n  font-family: 'Poppins', sans-serif;\r\n  font-weight: bold;\r\n  color: #ff6363;\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.intro p {\r\n  font-size: 1.2rem;\r\n  font-family: 'Roboto', sans-serif;\r\n  line-height: 1.8;\r\n  color: #e0e0e0;\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  text-align: center;\r\n}\r\n\r\n/* Section galerie */\r\n.image-gallery .gallery {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 20px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.image-gallery img {\r\n  border-radius: 8px;\r\n}\r\n\r\n/* Bande technologique */\r\n.technology-strip {\r\n  margin-top: 30px;\r\n  background-color: #353535;\r\n  padding: 20px 0;\r\n  border-radius: 15px;\r\n  overflow: hidden;\r\n  position: relative;\r\n  height: 80px;\r\n}\r\n\r\n.tech-container {\r\n  display: flex;\r\n  gap: 20px;\r\n  will-change: transform;\r\n}\r\n\r\n.tech-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  min-width: 100px;\r\n  text-align: center;\r\n}\r\n\r\n.tech-item img {\r\n  width: 40px;\r\n  height: 40px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.tech-item span {\r\n  color: #fff;\r\n  font-size: 0.9rem;\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EAyBXA,KAAK,EAAC;AAAkB;;EAC1BA,KAAK,EAAC,gBAAgB;EAACC,GAAG,EAAC;;mBA3BtC;;uBACEC,mBAAA,CAiCM,OAjCNC,UAiCM,GAhCJC,mBAAA,0BAA6B,E,0BAC7BC,mBAAA,CAUU;IAVDL,KAAK,EAAC;EAAO,IACpBK,mBAAA,CAA8D,YAA1D,uDAAqD,GACzDA,mBAAA,CAOI,WAPD,uYAOH,E,sBAGFD,mBAAA,6BAAgC,E,0BAfpCE,kBAAA,qYAyB