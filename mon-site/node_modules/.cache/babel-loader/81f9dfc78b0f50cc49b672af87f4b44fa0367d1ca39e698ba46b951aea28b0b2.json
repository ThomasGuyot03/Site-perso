 des technologies -->\r\n    <section class=\"technology-strip\">\r\n      <div class=\"tech-container\">\r\n        <div v-for=\"(tech, index) in visibleTechnologies\" :key=\"index\" class=\"tech-item\">\r\n          <img :src=\"tech.src\" :alt=\"tech.name\" />\r\n          <span>{{ tech.name }}</span>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, onMounted } from \"vue\";\r\n\r\n// Technologies à afficher\r\nconst technologies = [\r\n  { name: \"HTML\", src: \"https://cdn-icons-png.flaticon.com/512/732/732212.png\" },\r\n  { name: \"CSS\", src: \"https://cdn-icons-png.flaticon.com/512/732/732190.png\" },\r\n  { name: \"Bulma\", src: \"https://i.postimg.cc/m1Hxh2zL/icons8-bulma-io-48.png\" },\r\n  { name: \"Vue.js\", src: \"https://i.postimg.cc/8sPjs8zh/icons8-vue-js-48.png\" },\r\n  { name: \"Node.js\", src: \"https://cdn-icons-png.flaticon.com/512/5968/5968322.png\" },\r\n  { name: \"Python\", src: \"https://cdn-icons-png.flaticon.com/512/5968/5968350.png\" },\r\n];\r\n\r\n// Liste visible des technologies\r\nconst visibleTechnologies = reactive([]);\r\n\r\n// Fonction pour dupliquer les éléments pour un défilement continu\r\nconst duplicateItems = () => {\r\n  // Répéter les éléments 10 fois\r\n  for (let i = 0; i < 10; i++) {\r\n    visibleTechnologies.push(...technologies);\r\n  }\r\n};\r\n\r\nonMounted(() => {\r\n  duplicateItems();\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  margin-top: 60px;\r\n  padding: 20px;\r\n  color: #fff;\r\n  text-align: center;\r\n}\r\n\r\n/* Section introduction */\r\n.intro {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.intro h1 {\r\n  font-size: 3rem;\r\n  font-family: 'Poppins', sans-serif;\r\n  font-weight: bold;\r\n  color: #ff6363;\r\n  margin-bottom: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.intro p {\r\n  font-size: 1.2rem;\r\n  font-family: 'Roboto', sans-serif;\r\n  line-height: 1.8;\r\n  color: #e0e0e0;\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  text-align: center;\r\n}\r\n\r\n/* Section galerie */\r\n.image-gallery .gallery {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 20px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.image-gallery img {\r\n  border-radius: 8px;\r\n}\r\n\r\n/* Bande technologique */\r\n.technology-strip {\r\n  margin-top: 30px;\r\n  background-color: #353535;\r\n  padding: 20px 0;\r\n  border-radius: 15px;\r\n  overflow: hidden;\r\n  position: relative;\r\n  width: 50%; /* Réduit la bande à 50% de la largeur de l'écran */\r\n  margin: 0 auto;\r\n  height: 80px;\r\n}\r\n\r\n.tech-container {\r\n  display: flex;\r\n  gap: 20px;\r\n  position: relative;\r\n  animation: scroll 20s linear infinite; /* Animation de défilement */\r\n  width: 100%; /* Assurez-vous que la largeur est de 100% pour qu'il n'y ait pas de débordement */\r\n}\r\n\r\n.tech-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  min-width: 100px;\r\n  text-align: center;\r\n  position: relative;\r\n}\r\n\r\n.tech-item img {\r\n  width: 40px;\r\n  height: 40px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.tech-item span {\r\n  color: #fff;\r\n  font-size: 0.9rem;\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n/* Animation pour le défilement */\r\n@keyframes scroll {\r\n  0% {\r\n    transform: translateX(0);\r\n  }\r\n  100% {\r\n    transform: translateX(-100%); /* S'assurer que l'animation déplace les logos jusqu'à la fin du conteneur */\r\n  }\r\n}\r\n\r\n\r\n</style>\r\n\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EAyBXA,KAAK,EAAC;AAAkB;;EAC1BA,KAAK,EAAC;AAAgB;mBA3BjC;;uBACEC,mBAAA,CAiCM,OAjCNC,UAiCM,GAhCJC,mBAAA,0BAA6B,E,0BAC7BC,mBAAA,CAUU;IAVDJ,KAAK,EAAC;EAAO,IACpBI,mBAAA,CAA8D,YAA1D,uDAAqD,GACzDA,mBAAA,CAOI,WAPD,uYAOH,E,sBAGFD,mBAAA,6BAAgC,E,0BAfpCE,kBAAA,qYAyBIF,mBAAA,8BAAiC,EACjCC,mBAAA,CAOU,WAPVE,UAOU,GANRF,mBAAA,CAKM,OALNG,UAKM,I,kBAJJN,mBAAA,CAGMO,SAAA,QA/BdC,WAAA,CA4BqCC,MAAA,CAAAC,mBAAmB,EA5BxD,CA4BqBC,IAAI,EAAEC,KAAK;y